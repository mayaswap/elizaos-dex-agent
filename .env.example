# =====================================
# ElizaOS DEX Trading Agent Configuration
# =====================================

# === Core DEX Configuration ===
DEXSCREENER_API_KEY=your_dexscreener_api_key_here
GRAPHQL_ENDPOINT=https://graph.pulsechain.com/subgraphs/name/pulsechain/pulsex
DEFAULT_WALLET_PRIVATE_KEY=your_wallet_private_key_here

# === ElizaOS Platform Integration Tokens ===
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
DISCORD_APPLICATION_ID=your_discord_application_id_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here
TWITTER_USERNAME=your_twitter_username_here
TWITTER_PASSWORD=your_twitter_password_here

# === Database Configuration ===
# PostgreSQL (Production Recommended)
POSTGRES_URL=postgresql://username:password@localhost:5432/elizaos
# OR SQLite (Development/Testing)
SQLITE_DATA_DIR=./database

# === AI/LLM Configuration ===
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# === Agent Configuration ===
AGENT_NAME=DEX Master
AGENT_USERNAME=dexmaster

# === Security Settings ===
ENCRYPTION_KEY=your_32_character_encryption_key_here
JWT_SECRET=your_jwt_secret_here

# === Performance Settings ===
MAX_CONCURRENT_TRADES=5
DEFAULT_SLIPPAGE_TOLERANCE=0.5
DEFAULT_GAS_PRICE_GWEI=20

# === Monitoring & Logging ===
LOG_LEVEL=info
SENTRY_DSN=your_sentry_dsn_for_error_tracking

# === Feature Flags ===
ENABLE_TELEGRAM=true
ENABLE_DISCORD=true
ENABLE_WEB=true
ENABLE_TWITTER=false 